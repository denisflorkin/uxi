{
  "name": "uxi-docs",
  "version": "0.0.1",
  "description": "doc for cluedin-ui",
  "main": "./server/index.js",
  "scripts": {
    "start": "node .",
    "start:dev": "webpack-dev-server --config webpack.config.js",
    "dev": "uxi-cli dev",
    "dev2": "webpack-dev-server --config node_modules/uxi-cli/webpack.dev.config.js --port 3100",
    "buildAndBundle": "npm run build && npm run bundle",
    "bundle": "webpack --config webpack.PROD.config.js && npm run bundle:copy-index",
    "build": "npm run build:babel && npm run build:copy-files",
    "build:babel": "cross-env NODE_ENV=release babel ./src --out-dir ./build --ignore test.js,__tests__,__test__ --copy-files",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "clean:build": "rimraf build",
    "bundle:copy-index": " cp index.html dist/",
    "deploy-doc": "npm run buildAndBundle && echo '\n\n # now: git push uxilab\n\n' "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "highlight.js": "^9.12.0",
    "history": "^4.7.2",
    "marked": "^0.3.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-docgen": "^2.20.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "styled-components": "^3.2.0",
    "uxi-cli": "^1.0.56"
  },
  "devDependencies": {
    "cross-env": "^5.1.3"
  }
}
