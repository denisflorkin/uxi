    "build": "npm run lint && npm run test && npm run build:babel && npm run build:copy-files",
    "build:babel": "./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=release ./node_modules/.bin/babel ./src --out-dir ./build",
    "build:copy-files": "./node_modules/.bin/babel-node ./scripts/copy-files.js",
    "clean:build": "./node_modules/.bin/rimraf build",
    "test": "./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=test ./node_modules/.bin/jest",
    "test:coverage": "./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=test ./node_modules/.bin/jest --coverage",
    "test:watch": "./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=test ./node_modules/.bin/jest --watch",
    "typeinstall": "flow-typed install"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
